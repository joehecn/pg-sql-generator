{
  "name": "pg-sql-generator",
  "version": "0.0.1",
  "description": "postgresql generator",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joehecn/pg-sql-generator.git"
  },
  "keywords": [
    "postgresql",
    "sql",
    "generator"
  ],
  "author": "leanbrown@live.cn",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joehecn/pg-sql-generator/issues"
  },
  "homepage": "https://github.com/joehecn/pg-sql-generator#readme",
  "files": [
    "lib/**/*"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.0.4"
  }
}